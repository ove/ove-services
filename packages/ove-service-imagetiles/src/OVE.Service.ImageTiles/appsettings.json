{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImageStorageConfig": {
    "BasePath": "wwwroot/ImageStore/"
  },
  "ImageProcessingConfig": {
    "MaxConcurrent": 2,
    "PollSeconds": 30
  },
  "ASyncUploader": {
    "MaxConcurrent": 2,
    "PollSeconds": 30
  },
  "Service": {
    "Name": "Images",
    "FileTypes": [".png", ".jpg"],
    "ProcessingStates": {
      "0": "Unprocessed",
      "1": "Processing",
      "2": "Processed"
    }
  },
  "RegistrationUrl" : "http://localhost:8181/api/ServicesRegistry/Register" 
}
