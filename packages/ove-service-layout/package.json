{
  "name": "ove-service-layout",
  "private": true,
  "description": "The layout service",
  "license": "MIT",
  "version": "0.2.1",
  "author": "Ovidiu Serban, o.serban@imperial.ac.uk",
  "homepage": "https://github.com/ove/ove-services#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ove/ove-services.git"
  },
  "bugs": {
    "url": "https://github.com/ove/ove-services/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js --color",
    "start:dev": "nodemon -e yaml",
    "start:mockup": "json-server --watch mockup/db.json --routes mockup/routes.json --port 3004",
    "clean": "npx rimraf dist",
    "build": "babel src --out-dir dist",
    "build:clean": "npm run clean && npm run build",
    "lint": "eslint src/**",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test:benchmark": "node benchmark/benchmark.js"
  },
  "dependencies": {
    "@ove-lib/utils": "0.5.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "node-fetch": "2.6.1",
    "swagger-ui-express": "4.1.6",
    "underscore": "1.13.1",
    "validate.js": "0.13.1",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.14.2",
    "@babel/plugin-proposal-export-namespace-from": "7.14.2",
    "@babel/plugin-proposal-function-sent": "7.12.13",
    "@babel/plugin-proposal-json-strings": "7.14.2",
    "@babel/plugin-proposal-numeric-separator": "7.14.2",
    "@babel/plugin-proposal-throw-expressions": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/preset-env": "7.14.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "benchmark": "2.1.4",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "jest-coverage-badges": "1.1.2",
    "json-server": "0.16.3",
    "microtime": "3.0.0",
    "nodemon": "2.0.7",
    "supertest": "6.1.3",
    "sync-request": "6.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**.js",
      "!src/.eslintrc.js",
      "!src/index.js",
      "!src/routes/**.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
