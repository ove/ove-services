{
  "name": "ove-service-layout",
  "private": true,
  "description": "The layout service",
  "license": "MIT",
  "version": "0.1.1",
  "author": "Ovidiu Serban, o.serban@imperial.ac.uk",
  "homepage": "https://github.com/ove/ove-services#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ove/ove-services.git"
  },
  "bugs": {
    "url": "https://github.com/ove/ove-services/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js --color",
    "start:dev": "nodemon -e yaml",
    "start:mockup": "json-server --watch mockup/db.json --routes mockup/routes.json --port 3004",
    "clean": "npx rimraf dist",
    "build": "babel src --out-dir dist",
    "build:clean": "npm run clean && npm run build",
    "lint": "eslint src/**",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test:benchmark": "node benchmark/benchmark.js"
  },
  "dependencies": {
    "@ove-lib/utils": "0.4.0",
    "body-parser": "1.19.0",
    "cors": "latest",
    "express": "4.16.4",
    "http-status-codes": "1.3.2",
    "node-fetch": "2.3.0",
    "swagger-ui-express": "4.0.2",
    "underscore": "1.9.1",
    "validate.js": "0.12.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "benchmark": "2.1.4",
    "eslint": "5.16.0",
    "eslint-plugin-jest": "22.4.1",
    "jest": "24.7.1",
    "jest-coverage-badges": "1.1.2",
    "json-server": "0.14.2",
    "microtime": "3.0.0",
    "nodemon": "1.18.10",
    "supertest": "4.0.2",
    "sync-request": "6.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**.js",
      "!src/.eslintrc.js",
      "!src/index.js",
      "!src/routes/**.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
